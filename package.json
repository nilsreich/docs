{
  "dependencies": {
    "eslint-config-next": "^13.0.3",
    "next": "^13.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "dev:tailwind": "concurrently \"next dev --turbo\" \"npm run tailwind -- --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i app/tailwind.css -o app/globals.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "npm run tailwind"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4"
  }
}
